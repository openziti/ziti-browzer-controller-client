{
  "name": "@openziti/ziti-browzer-edge-client",
  "version": "0.1.10",
  "description": "A Ziti Edge REST Client for JavaScript, autogenerated from the Ziti Controller's Swagger spec",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist spec && gulp clean",
    "fetchYaml": "node src/fetchYaml.js",
    "ymlToJson": "node src/ymlToJson.js --input spec/client.yml --output spec/client.json",
    "clientGenerator": "node src/clientGenerator.js",
    "gulp": "gulp build",
    "build": "npm-run-all clean fetchYaml ymlToJson clientGenerator gulp",
    "test": "karma start karma.conf.cjs"
  },
  "bugs": {
    "url": "https://github.com/openziti/ziti-browzer-edge-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/openziti/ziti-browzer-edge-client.git"
  },
  "keywords": [
    "swagger",
    "rest",
    "ziti",
    "ziti edge",
    "zero trust",
    "zero-trust",
    "browZer",
    "browser",
    "js",
    "javascript",
    "netfoundry"
  ],
  "author": {
    "name": "NetFoundry",
    "url": "http://netfoundry.io",
    "email": "openziti@netfoundry.io"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/openziti/ziti-browzer-edge-client",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "@open-wc/karma-esm": "^4.0.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/node": "^17.0.21",
    "@types/superagent": "^4.1.15",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "common-tags": "^1.8.0",
    "del": "^6.0.0",
    "globby": "^11.0.1",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^8.0.0",
    "https": "^1.0.0",
    "js-beautify": "^1.14.0",
    "jshint": "^2.13.4",
    "karma": "^6.3.17",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "mocha": "^9.2.2",
    "mustache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.2",
    "stream": "^0.0.2",
    "typescript": "^4.6.2",
    "url": "^0.11.0",
    "yaml": "^1.10.2",
    "yargs": "^17.3.1"
  },
  "dependencies": {
    "q": "^1.5.1",
    "superagent": "^7.1.1"
  },
  "babel": {
    "plugins": [
      "dynamic-import-node"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  }
}