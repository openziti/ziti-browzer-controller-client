  "homepage": "https://github.com/openziti/ziti-browzer-edge-client",
  "devDependencies": {
    "js-beautify": "^1.14.0",
    "jshint": "^2.13.4",
    "mustache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.2",
    "yaml": "^1.10.2",
